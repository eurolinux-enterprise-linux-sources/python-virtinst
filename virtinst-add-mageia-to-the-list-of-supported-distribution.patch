(cherry picked from commit 763c9b92869aaceec149572cd398f310204cdfc6)
---
 virtinst/osdict.py | 12 ++++++++++++
 1 file changed, 12 insertions(+)

diff --git a/virtinst/osdict.py b/virtinst/osdict.py
index b57e274..f0f9918 100644
--- a/virtinst/osdict.py
+++ b/virtinst/osdict.py
@@ -417,6 +417,18 @@ OS_TYPES = {
         },
     },

+    "mageia1": {
+        "label": "Mageia 1 and later",
+        "distro": "mageia",
